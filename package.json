{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "webpack --mode=production --node-env=production",
    "start": "next start",
    "lint": "next lint",
    "coverage": "nyc yarn",
    "clear": "rimraf lib",
    "build8": "webpack --mode=development",
    "build7": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "teste1": "webpack /**/*spote/testes/.spec.js --require-babel-register --watch ",
    "teste2": "./node_modules/.bin/smoke/**/*spote/smoke/testes/.spec.js",
    "tdd": "./node_modules/.bin/mocha /**/* spote/mocha/testes/.spec.js --watch",
    "serve": "webpack serve",
    "prepare": "husky install",
    "build2": "./node_modules/.bin/babel --out-dir lib src",
    "build3": "./node_modules/.bin/webpack --output-filename  spote.umd.js --mode=production ",
    "build4": "./node_modules/.bin/webpack --output-filename  spote.umd.min.js --mode=production ",
    "build5": "yarn build2 && yarn build3 &&  yarn build4",
    "build6": "./node_modules/.bin/webpack server --mode=development ",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production"
  },
  "dependencies": {
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
