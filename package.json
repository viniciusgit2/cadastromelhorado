    "@webpack-cli/generators": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^9.1.2",
    "chai": "^4.3.7",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
