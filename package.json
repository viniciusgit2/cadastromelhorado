{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    
    "dev": "next dev",
    "build": "webpack --mode=production --node-env=production",
    "start": "next start",
    "lint": "next lint",
    "coverage": "nyc yarn",
    "clear": "rimraf lib",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "teste1": "./node_modules/.bin/mocha /**/* spote/mocha/testes/.spec.js --require babel-register  ",
    "teste2": "./node_modules/.bin/smoke /**/* spote/smoke/testes/.spec.js",
    "tdd": "./node_modules/.bin/mocha /**/* spote/mocha/testes/.spec.js --watch",
    "serve": "webpack serve",
    "prepare": "husky install",
    "build2": "./node_modules/.bin/babel --out-dir lib src",
    "build3":" yarn build2 --watch",
    "build4": "./node_modules/.bin/webpack --output-filename  spote.umd.js --mode=production ",
    "build5": "./node_modules/.bin/webpack --output-filename  spote.umd.min.js --mode=production ",
    "build6": "yarn build2 && yarn build3 &&  yarn build4"
  },
  "dependencies": {
    "@next/font": "13.1.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "next": "13.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@webpack-cli/generators": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.2",
    "chai": "^4.3.7",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.2",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0",
    "sinon-stub-promise": "^4.0.0",
    "smoke": "^3.1.1",
    "source-map": "^0.7.4",
    "style-loader": "^3.3.1",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.1.0",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "description": "My webpack project"
}
